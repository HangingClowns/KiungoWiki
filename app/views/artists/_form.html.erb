<ul>
  <li>
    <%= form.label :work_title %>
    <%= form.fields_for :work_wiki_link do |work_link_form| %>
      <%= work_link_form.text_field :encoded_link, :class=>"work_title_lookup", :"data-pre"=>[work_link_form.object.combined_link].to_json %>
    <% end %>
  </li>

  <li>
    <%= form.label :recording_date %>
    <%= form.text_field :recording_date %>
  </li>

  <li>
    <%= form.label :recording_location %>
    <%= form.text_field :recording_location %>
  </li>

  <li>
    <%= form.label :duration %>
    <%= form.text_field :duration %>
  </li>

  <li>
    <%= form.label :rythm %>
    <%= form.text_field :rythm %>
  </li>
</ul>

