<dl>
  <dt><%= form.label :work_title %></dt>
  <dd><%= form.fields_for :work_wiki_link do |work_link_form| %>
        <%= work_link_form.text_field :reference_text, :class=>"work_title_lookup", :"data-pre"=>[work_link_form.object.combined_link].to_json %>
      <% end %></dd>

  <dt><%= form.label :recording_date %></dt>
  <dd><%= form.text_field :recording_date %></dd>

  <dt><%= form.label :recording_location %></dt>
  <dd><%= form.text_field :recording_location %></dd>

  <dt><%= form.label :duration %></dt>
  <dd><%= form.text_field :duration %></dd>

  <dt><%= form.label :rythm %></dt>
  <dd><%= form.text_field :rythm %></dd>

  <dt><%= form.label :artist_wiki_links_text %></dt>
  <dd><%= form.text_field :artist_wiki_links_text, :class=>"artists_lookup", :"data-pre"=>form.object.artist_wiki_links_combined_links.to_json %></dd>

  <dt><%= form.label :album_wiki_links_text %></dt>
  <dd><%= form.text_field :album_wiki_links_text, :class=>"album_title_lookup", :"data-pre"=>form.object.album_wiki_links_combined_links.to_json %></dd>

</dl>

